local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Test Key system", HidePremium = false, SaveConfig = true, IntroEnabled = false})

OrionLib:MakeNotification({
  Name = "Logged In!",
  Content = "You are logged in as "..Player.Name..".",
  Image = "rbxassetid://4483345998",
  Time = 5
})

_G.Key = "JustANormalKey"
_G.KeyInput = "string"

function Justatest()
    print("correct")
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		  _G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      if _G.key Input == _G.Key  then
      Justatest()
  	end    
})

  
